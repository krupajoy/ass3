#include<stdio.h>
int main()
{
	int n,k,esum,osum;
	int i,cnt=0;
	cnt=esum=osum=0;
	scanf("%d%d",&n,&k);
	i=1;
	while(i<=n){
		if(i%k==0){
			printf("%d",i);
			cnt++;
			if(i%2==0)esum=esum+i;
			else
			osum+=i;
		}
		i++;
	}
	printf("\ncount=%d",cnt);
	printf("\nevensum=%d",esum);
	printf("\noddsum=%d",osum);
}
